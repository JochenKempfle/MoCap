<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="MoCapFrame">
		<title>Motion Capturing</title>
		<centered>1</centered>
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="FrameSizer" member="yes">
			<object class="sizeritem">
				<object class="wxPanel" name="ID_MAINPANEL" variable="MainPanel" member="yes">
					<object class="wxBoxSizer" variable="MainSizer" member="yes">
						<object class="sizeritem">
							<object class="wxBoxSizer" variable="BoxSizer2" member="no">
								<orient>wxVERTICAL</orient>
								<object class="sizeritem">
									<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
										<label>View Mode</label>
										<orient>wxVERTICAL</orient>
										<object class="sizeritem">
											<object class="wxButton" name="ID_BUTTONSENSORDETAIL" variable="ButtonSensorDetail" member="yes">
												<label>Sensor Detail</label>
												<default>1</default>
												<handler function="OnButtonSensorDetailClick" entry="EVT_BUTTON" />
											</object>
											<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxButton" name="ID_BUTTONVISUAL" variable="ButtonVisual" member="yes">
												<label>Visual</label>
												<hidden>1</hidden>
												<handler function="OnButtonVisualClick" entry="EVT_BUTTON" />
											</object>
											<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxButton" name="ID_BUTTONFULLSCREEN" variable="ButtonFullScreen" member="yes">
												<label>Full Screen</label>
												<handler function="OnButtonFullScreenClick" entry="EVT_BUTTON" />
											</object>
											<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
										</object>
									</object>
									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
								</object>
								<object class="spacer">
									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
									<option>1</option>
								</object>
								<object class="sizeritem">
									<object class="wxButton" name="ID_BUTTONSTART" variable="ButtonStart" member="yes">
										<label>Start</label>
										<hidden>1</hidden>
										<handler function="OnButtonStartClick" entry="EVT_BUTTON" />
									</object>
									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
								</object>
								<object class="sizeritem">
									<object class="wxButton" name="ID_BUTTONSTOP" variable="ButtonStop" member="yes">
										<label>Stop</label>
										<hidden>1</hidden>
										<handler function="OnButtonStopClick" entry="EVT_BUTTON" />
									</object>
									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
								</object>
								<object class="sizeritem">
									<object class="wxButton" name="ID_BUTTONAUTOASSIGN" variable="ButtonAutoAssign" member="yes">
										<label>Auto Assign</label>
										<hidden>1</hidden>
										<handler function="OnButtonAutoAssignClick" entry="EVT_BUTTON" />
									</object>
									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
								</object>
								<object class="sizeritem">
									<object class="wxButton" name="ID_BUTTONCALIBRATE" variable="ButtonCalibrate" member="yes">
