<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="TimelinePanel">
		<bg>#787878</bg>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<style>wxTAB_TRAVERSAL|wxFULL_REPAINT_ON_RESIZE</style>
		<handler function="OnPaint" entry="EVT_PAINT" />
		<handler function="OnKillFocus" entry="EVT_KILL_FOCUS" />
		<handler function="OnLeftDown" entry="EVT_LEFT_DOWN" />
		<handler function="OnLeftUp" entry="EVT_LEFT_UP" />
		<handler function="OnMouseMove" entry="EVT_MOTION" />
		<handler function="OnMouseEnter" entry="EVT_ENTER_WINDOW" />
		<handler function="OnMouseLeave" entry="EVT_LEAVE_WINDOW" />
		<handler function="OnMouseWheel" entry="EVT_MOUSEWHEEL" />
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="spacer">
				<flag>wxALL|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<size>100,10</size>
			</object>
			<object class="sizeritem">
				<object class="wxButton" name="ID_BUTTONCLEAR" variable="ButtonClear" member="yes">
					<label>Clear All</label>
					<size>-1,26</size>
				</object>
				<flag>wxALL|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<border>2</border>
			</object>
			<object class="sizeritem">
				<object class="wxButton" name="ID_BUTTONREMOVE" variable="ButtonRemove" member="yes">
					<label>Remove</label>
					<size>-1,26</size>
				</object>
				<flag>wxALL|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<border>2</border>
			</object>
			<object class="sizeritem">
				<object class="wxButton" name="ID_BUTTONCUT" variable="ButtonCut" member="yes">
					<label>Cut</label>
					<size>-1,26</size>
				</object>
				<flag>wxALL|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<border>2</border>
			</object>
			<object class="spacer">
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxButton" name="ID_BUTTONZOOMIN" variable="ButtonZoomIn" member="yes">
					<label>Zoom +</label>
					<size>-1,26</size>
					<handler function="OnButtonZoomInClick" entry="EVT_BUTTON" />
				</object>
				<flag>wxALL|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<border>2</border>
			</object>
			<object class="sizeritem">
				<object class="wxButton" name="ID_BUTTONZOOMOUT" variable="ButtonZoomOut" member="yes">
					<label>Zoom -</label>
					<size>-1,26</size>
					<handler function="OnButtonZoomOutClick" entry="EVT_BUTTON" />
				</object>
				<flag>wxALL|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<border>2</border>
			</object>
		</object>
	</object>
</wxsmith>
